{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/meet/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/meet/manifest.json",
      "dest": "/meet/manifest.json"
    },
    {
      "src": "/meet/(.*)",
      "dest": "/meet/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}